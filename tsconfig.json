{
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "components/*": ["./src/components/*"],
      "pages/*": ["./src/pages/*"],
      "assets/*": ["src/assets/*"],
      "forms/*": ["src/forms/*"],
      "providers/*": ["src/providers/*"],
      "utils/*": ["src/utils/*"],
      "libs/*": ["src/libs/*"],
      "store/*": ["src/store/*"],
      "constants/*": ["src/constants/*"],
      "hooks/*": ["src/hooks", "src/hooks/*"]
    }
  },
  "include": ["src"]
}
